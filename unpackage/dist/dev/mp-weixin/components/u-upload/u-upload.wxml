<block wx:if="{{!disabled}}"><view class="u-upload data-v-10a201e9"><block wx:for="{{lists}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{showUploadList}}"><view class="u-list-item u-preview-wrap data-v-10a201e9" style="{{'width:'+(width+'rpx')+';'+('height:'+(width+'rpx')+';')}}"><block wx:if="{{deletable}}"><view data-event-opts="{{[['tap',[['deleteItem',[index]]]]]}}" class="u-delete-icon data-v-10a201e9" catchtap="__e"><cl-icon vue-id="{{'4ff0f762-1-'+index}}" name="cl-icon-close" color="#ffffff" class="data-v-10a201e9" bind:__l="__l"></cl-icon></view></block><block wx:if="{{showProgress&&item.progress>0&&!item.error}}"><u-line-progress class="u-progress data-v-10a201e9" vue-id="{{'4ff0f762-2-'+index}}" show-percent="{{false}}" height="16" percent="{{item.progress}}" bind:__l="__l"></u-line-progress></block><block wx:if="{{item.error}}"><view data-event-opts="{{[['tap',[['retry',[index]]]]]}}" class="u-error-btn data-v-10a201e9" catchtap="__e">点击重试</view></block><block wx:if="{{!item.isImage}}"><image class="u-preview-image data-v-10a201e9" src="{{item.url||item.path}}" mode="{{imageMode}}" data-event-opts="{{[['tap',[['doPreviewImage',[item.url||item.path,index]]]]]}}" catchtap="__e"></image></block></view></block></block><scoped-slots-file file="{{lists}}" bind:__l="__l"></scoped-slots-file><block wx:if="{{maxCount>lists.length}}"><view data-event-opts="{{[['tap',[['selectFile',['$event']]]]]}}" style="display:inline-block;" bindtap="__e" class="data-v-10a201e9"><slot name="addBtn"></slot><block wx:if="{{!customBtn}}"><view class="u-list-item u-add-wrap data-v-10a201e9" style="{{'width:'+(width+'rpx')+';'+('height:'+(width+'rpx')+';')}}" hover-class="u-add-wrap__hover" hover-stay-time="150"><view class="iconfont add-icon u-icon data-v-10a201e9"></view></view></block></view></block></view></block>